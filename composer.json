{
    "name": "flagbit/magento2-transaction-mail-extender",
    "description": "Extends Magento 2 transaction mails with a Schema.org conform HTML content",
    "type": "magento2-module",
    "authors": [
        {
            "name": "Michael Vogel",
            "email": "michael.vogel@flagbit.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/flagbit/magento2-transaction-mail-extender",
    "keywords": [
        "magento",
        "magento2",
        "transaction",
        "mail",
        "extender",
        "schema.org"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "flagbit/schemaorg-email-body": "^0.2.0",
        "magento/framework": "^103.0",
        "magento/magento-composer-installer": "^0.1.13",
        "magento/module-backend": "^102.0",
        "magento/module-sales": "^103.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "EinsUndEins\\TransactionMailExtender\\": "",
            "Magento\\Framework\\": "htdocs/lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "htdocs/setup/src/Magento/Setup/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
